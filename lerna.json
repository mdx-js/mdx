{
  "$schema": "http://json.schemastore.org/lerna",
  "version": "2.0.0-next.1",
  "packages": [
    "packages/*"
  ],
  "npmClient": "yarn",
  "useWorkspaces": true,
  "command": {
    "publish": {
      "ignoreChanges": [
        "**/docs/**",
        "**/examples/**",
        "**/test/**",
        "**/*.md"
      ]
    }
  }
}
